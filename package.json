    "auth",
    "api"
  ],
  "dependencies": {
    "glue": "^4.1.0",
    "good": "^7.1.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.1.1",
    "hapi-auth-jwt2": "^7.2.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "blipp": "^2.3.0",
    "code": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "lab": "^13.0.4",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2"
  },
  "engines": {
    "node": ">=4.0.0"
  }
